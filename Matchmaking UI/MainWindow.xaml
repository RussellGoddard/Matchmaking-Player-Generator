<Window x:Class="Matchmaking_UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Matchmaking_UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" Loaded="OnLoad">
    <Grid>
        <TextBox x:Name="text_Input" HorizontalAlignment="Left" Height="23" Margin="19,26,0,0" TextWrapping="Wrap" Text="Input" VerticalAlignment="Top" Width="188" FontFamily="Lucida Sans Typewriter"/>
        <TextBox x:Name="text_Output" HorizontalAlignment="Left" Height="175" Margin="212,43,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="285" FontFamily="Lucida Sans Typewriter"/>
        <TextBox x:Name="text_3" HorizontalAlignment="Left" Height="23" Margin="19,95,0,0" TextWrapping="Wrap" Text="Counter" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="text_3_Copy" HorizontalAlignment="Left" Height="23" Margin="19,143,0,0" TextWrapping="Wrap" Text="Counter" VerticalAlignment="Top" Width="120"/>
        <Button Content="Get CPP Count" HorizontalAlignment="Left" Margin="19,179,0,0" VerticalAlignment="Top" Width="120" Click="Button_Click"/>
        <Button x:Name="Button_MakePlayers" Content="Make Players" HorizontalAlignment="Left" Margin="19,62,0,0" VerticalAlignment="Top" Width="122" Click="Button_MakePlayers_Click"/>
        <Button x:Name="Button_GetDistro" Content="Get Distribution" HorizontalAlignment="Left" Margin="212,234,0,0" VerticalAlignment="Top" Width="99" Click="Button_GetDistro_Click"/>

    </Grid>
</Window>
